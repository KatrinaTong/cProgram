@model MoneyBMineWP.Models.NYSEDailyPriceA

@{
    ViewBag.Title = "Add new data for the foreign stock database";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div  align="right">
        @Html.ActionLink("Back to Whole Data List", "Index")
    </div>

    <div class="form-horizontal">

        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
